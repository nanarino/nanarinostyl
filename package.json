    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nanarino/nanarinostyl/issues"
    },
    "peerDependencies": {
        "stylus": ">=0.64.0"
    },
    "devDependencies": {
        "@astrojs/mdx": "4.0.4",
        "@holy-two/data-theme": "^0.0.6",
        "@types/node": "^22.10.5",
        "astro": "^5.1.4",
        "astro-icon": "^1.1.5",
        "copy-to-clipboard": "^3.3.3",
        "css-doodle": "^0.34.11",
        "csso-cli": "^4.0.2",
        "dedent": "^1.5.3",
        "rm-all": "^1.1.1",
        "smooth-corners": "^1.0.8",
        "stylus": "^0.64.0"
